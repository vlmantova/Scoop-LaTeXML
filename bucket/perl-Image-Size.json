{
    "version": "3.300",
    "description": "Image::Size - Determine the size of images in several common formats",
    "homepage": "https://metacpan.org/dist/Image-Size",
    "license": "Artistic-1.0-Perl|GPL-1.0-only",
    "url": "https://cpan.metacpan.org/authors/id/R/RJ/RJRAY/Image-Size-3.300.tar.gz#/Image-Size",
    "hash": "53c9b1f86531cde060ee63709d1fda73cabc0cf2d581d29b22b014781b9f026b",
    "depends": "perl",
    "installer": {
        "script": [
            "Rename-Item (Join-Path $dir $fname) (Join-Path $dir \"$fname.tar.gz\")",
            "cpanm \"$(Join-Path $dir \"$fname.tar.gz\")\" -l \"$dir\" --verbose",
            "Remove-Item (Join-Path $dir \"$fname.tar.gz\")"
        ]
    },
    "bin": "bin\\imgsize.bat",
    "post_install": [
        "Add-Path -Path \"$original_dir\\lib\" -TargetEnvVar PERL5LIB -Global:$global"
    ],
    "pre_uninstall": [
        "Remove-Path -Path \"$dir\\lib\" -TargetEnvVar PERL5LIB -Global:$global"
    ]
}
